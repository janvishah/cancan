<h1>Create Article</h1>

<%= form_for @article, :url => articles_path do |f| %>
  <div>
          <%= label_tag :name %><br />
          <%= f.text_field :name %>
  </div>
  <div>
          <%= label_tag :description %><br />
          <%= f.text_area :description %>
  </div>
  <div><%= submit_tag "create" %></div>
<% end %>
</br>
</br>
<%= link_to "Logout",destroy_user_session_path, :method => :delete %>